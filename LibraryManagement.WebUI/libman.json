{
  "version": "3.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "marked@15.0.7",
      "destination": "wwwroot/lib/marked/"
    },
    {
      "provider": "jsdelivr",
      "library": "markdown@0.5.0",
      "destination": "wwwroot/lib/markdown/"
    },
    {
      "provider": "cdnjs",
      "library": "tailwindcss@2.2.19",
      "destination": "wwwroot/lib/tailwindcss/"
    }
  ]
}